@model AlgorytmDobieraniaZasobowProdukcyjnych.Models.DataTablePartialModel

<label>@Model.TableName</label>
<table style="font-size:0.9rem" ; text-align="justify" ,
       data-name="@Model.TableName" id="dataTable" class="table table-sm table-bordered table-hover table-responsive"
       width="100%" cellspacing="0">
    <thead class="thead-light">
        <tr>
            @foreach (var info in Model.Infos)
            {
                if (info.Name == "Kod")
                {
                    <th id="kod" scope="col">@info.Name</th>
                }
                else
                {
                    <th id="@info.Name" scope="col">@info.Name</th>
                }
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model.Datas)
        {
        <tr data-obraz="@data.Obraz" data-obraz2d="@data.Rys2D">
            @foreach (var info in Model.Infos)
            {
                    <td>@info.GetValue(data)</td> 
            }
        </tr>
        }
    </tbody>

</table>
