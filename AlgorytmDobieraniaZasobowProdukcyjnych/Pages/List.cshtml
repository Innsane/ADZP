@page
@model AlgorytmDobieraniaZasobowProdukcyjnych.Pages.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>Tokarki</h1>

<table class="table table-bordered table-sm">
    <thead class="thead-light">
        <tr>
            <th scope="col">Oznaczenie</th>
            <th scope="col">Typ</th>
            <th scope="col">Kod</th>
            <th scope="col">Producent</th>
        </tr>
    </thead>
    @foreach (var lathe in Model.Lathes)
    {
        <tbody>
            <tr>
                <td scope="row">@lathe.Oznaczenie</td>
                <td>@lathe.Typ</td>
                <td>@lathe.Kod</td>
                <td>@lathe.Producent</td>
        </tbody>
    }
</table>

